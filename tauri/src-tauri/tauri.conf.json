{
  "productName": "DocuScope CA",
  "version": "0.4.0",
  "identifier": "docuscope.streamlit.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "DocuScope CA",
        "width": 1400,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "label": "main",
        "visible": false,
        "dragDropEnabled": false
      },
      {
        "title": "DocuScope CA",
        "width": 1400,
        "height": 800,
        "label": "splashscreen",
        "url": "src/splashscreen.html"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "publisher": "David Brown",
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": [
      "binaries/docuscope"
    ]
  }
}